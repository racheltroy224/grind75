//this solutions runtime was 691 ms beat 8.77% of previous attempts on leetcode (need to come back and optimize)
//this solutions memory was 10.2 MB which bea 59.79% of previous attempts on leetcode

class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> solution;

        for(int i = 0; i < nums.size(); i++) {
            for(int j = i; j < nums.size(); j++) {
                if(nums[i] + nums[j] == target && i != j)
                {
                    solution.push_back(i);
                    solution.push_back(j);
                }
            }
        }
        return solution;
    }
};
